<% for event in @events %>
<% if profile %>
  You
<% else %>
  <%= @user.username %>
<% end %>
<% if event.target_object == @game_participation %>
  got
  <% case event.target_object.class.to_s %>
  <% when 'GameParticipation' %>
     bit by <%= event.game_participation.user.username %>
  <% end %>
<% else %>
<% case event.target_object.class.to_s %>
<% when 'GameParticipation' %>
   bit <%= event.target_object.user.username %>
<% when 'Vaccine' %>
  took a vaccine
<% when 'PseudoBite' %>
  took a psuedo_bite
<% when 'BiteShare' %>
  shared a bite with <%= event.target_object.shared_with.user.username %>
<% end %>
<% end %>
at <%= event.occured_at %>
<br />
<% end %>